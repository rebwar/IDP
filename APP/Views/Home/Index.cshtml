@model List<Contact>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table class="table table-bordered table-sm">
        <thead>
            <tr>
                <td>Name</td>
                <td>Owner Id</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@string.Format("{0} {1}", @item.FirstName, @item.LastName)</td>
                    <td>@item.OwnerId</td>
                </tr>

            }
        </tbody>
    </table>
</div>
